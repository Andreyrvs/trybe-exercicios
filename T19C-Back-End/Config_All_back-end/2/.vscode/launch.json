// launch.json
{
  "configurations": [
  {
		// Aqui você pode colocar o nome que preferir.
		// ao iniciar o debugger é o nome que ira aparecer
    "name": "dev",
    "request": "launch",
    "runtimeArgs": [
      "run-script",
      "dev"
    ],
    "runtimeExecutable": "npm",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "node"
  }
  ]
}